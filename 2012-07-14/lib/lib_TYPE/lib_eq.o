Module lib_eq Import "lib_logic";
$[Eq : {t|SET}t->t->Prop];
$[Eqr : {t|SET}{x:t}Eq%%t x x];
$[Eq_elim : {t|SET}{P:{x,y:t}(Eq%%t x y)->TYPE}({x:t}P x x (Eqr%%t x))->{x,y:t}{p:Eq%%t x y}P x y p];
[[t:SET][P:{x,y:t}(Eq x y)->TYPE][h:{x:t}P x x (Eqr x)][x:t]   Eq_elim P h x x (Eqr x)  ==>  h x]
;
[Eq_refl : {t|SET}refl%%t (Eq|t)
  = [t|SET]Eqr%%t];
[Eq_subst : {t|SET}{m,n|t}(Eq%%t m n)->{P:t->TYPE_minus1}(P m)->P n
  = [t|SET][m,n|t]Eq_elim|t ([x,y:t][_:Eq%%t x y]{P:t->TYPE_minus1}(P x)->P y) ([x:t][P:t->TYPE_minus1][H:P x]H) m n];
[trivType : TYPE_minus1
  = {T|TYPE_minus2}T->T];
[emptyType : TYPE_minus1
  = {T|TYPE_minus2}T];
Configure Theorems trivType emptyType Eq_subst;
